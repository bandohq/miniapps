{
  "compilerOptions": {
    /* Bundler mode */
    "allowJs": true,
    "moduleResolution": "bundler",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@config/*": [
        "config/*"
    ],
    "@pages/*": [
      "pages/*"
    ],
    "@layouts/*": [
      "layouts/*"
    ],
    "@components/*": [
      "components/*"
    ],
    "@translations/*": [
      "translations/*"
    ],
    "@assets/*": [
      "assets/*"
    ],
    "@styles/*": [
      "styles/*"
    ],
    "@hooks/*": [
      "hooks/*"
    ],
    "@store/*": [
      "store/*"
    ],
    "@helpers/*": [
        "helpers/*"
      ]
    },
    "types": [
      "@mui/material",
      "@mui/icons-material"
    ]
  },
  "include": ["src"]
}